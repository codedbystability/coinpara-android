{
  "name": "coinpara",
  "version": "3.1.4",
  "private": true,
  "scripts": {
    "android": "source ~/.bash_profile && npx react-native run-android",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "generate-logo": "npx react-native generate-bootsplash ./assets/images/app/logo.png --background-color=00254d   --logo-width=100 --assets-path=./assets --flavor=main",
    "svgr": "npx @svgr/cli@5.4.0 ./src/assets/icons -d ./src/components/page-components/svg-icons --native --icon --svg-props 'width=22,height=22'",
    "svgr-flags": "npx @svgr/cli@5.4.0 ./src/assets/flags -d ./src/components/page-components/flag-icons --native --icon --svg-props 'width=22,height=22'",
    "clean": "watchman watch-del-all && rm -rf node_modules && npm install && rm -rf /tmp/metro-* && npm run start --reset-cache",
    "just-clean": "watchman watch-del-all && rm -rf node_modules && yarn install"
  },
  "dependencies": {
    "@babel/preset-typescript": "7.16.7",
    "@intercom/intercom-react-native": "2.0.1",
    "@microsoft/signalr": "5.0.11",
    "@react-native-community/clipboard": "1.5.1",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/netinfo": "7.1.7",
    "@react-native-firebase/app": "^14.5.0",
    "@react-native-firebase/messaging": "^14.5.0",
    "@react-native-picker/picker": "2.2.0",
    "@react-navigation/bottom-tabs": "6.0.9",
    "@react-navigation/native": "6.0.6",
    "@react-navigation/stack": "6.0.11",
    "@reduxjs/toolkit": "1.6.2",
    "accounting": "^0.4.1",
    "axios": "0.24.0",
    "babel-preset-react-native": "2.1.0",
    "fbjs": "^3.0.2",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-background-timer": "^2.4.1",
    "react-native-big-list": "^1.5.2",
    "react-native-biometrics": "^2.1.4",
    "react-native-bootsplash": "4.1.2",
    "react-native-camera": "4.2.1",
    "react-native-dropdownalert": "4.5.1",
    "react-native-fast-image": "8.5.11",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-haptic-feedback": "^1.13.1",
    "react-native-idle-timer": "2.1.7",
    "react-native-image-picker": "4.1.2",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-mmkv": "1.6.3",
    "react-native-orientation": "3.1.3",
    "react-native-permissions": "^3.2.0",
    "react-native-qrcode-svg": "^6.1.0",
    "react-native-reanimated": "2.4.1",
    "react-native-recaptcha-that-works": "^1.2.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "3.11.1",
    "react-native-share": "7.3.5",
    "react-native-snap-carousel": "3.9.1",
    "react-native-sortable-list": "^0.0.24",
    "react-native-svg": "12.1.1",
    "react-native-webview": "11.14.2",
    "react-redux": "7.2.6",
    "reanimated-bottom-sheet": "1.0.0-alpha.22",
    "redux": "4.1.2",
